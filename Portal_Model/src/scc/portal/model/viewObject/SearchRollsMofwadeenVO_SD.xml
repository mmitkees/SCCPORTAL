  <SQLQuery>
    <![CDATA[SELECT DISTINCT  COURT_ROLL_MOFEDEEN.rule_date, 
Cases.rule_number, Cases.court_year, Cases.rule_subject_type_code,
getRulesSubjectTypeName(Cases.rule_subject_type_code) as rule_subject_type_name,
get_roll_status(COURT_ROLL_MOFEDEEN.Case_id,COURT_ROLL_MOFEDEEN.rule_date) as roll_status,
get_cases_claimeds_brief(Cases.case_id) as Claimeds ,
get_cases_defendants_brief(Cases.case_id) as defendents,
cases.subject, COURT_ROLL_MOFEDEEN.decree, 
get_judge_done_name_court(COURT_ROLL_MOFEDEEN.judge_Done) judge_Done, COURT_ROLL_MOFEDEEN.Postpone_Date,
COURT_ROLL_MOFEDEEN.role_order, 
getcourtname(  Cases.court_type_code)  as court_type_name,Cases.Case_Status_CODE ,
getCaseStatusName( Cases.Case_Status_CODE) as status_name 
FROM         COURT_ROLL_MOFEDEEN INNER JOIN
                      Cases ON COURT_ROLL_MOFEDEEN.Case_id = Cases.case_id LEFT OUTER JOIN
                      cases_claimed ON Cases.case_id = cases_claimed.case_id LEFT OUTER JOIN
                      cases_defendant ON Cases.case_id = cases_defendant.case_id LEFT OUTER JOIN
                      case_defendant_lawyers ON cases_defendant.case_id = case_defendant_lawyers.case_id 
                      AND  cases_defendant.citizen_id = case_defendant_lawyers.citizen_id LEFT OUTER JOIN
                      case_claimed_lawyers ON cases_claimed.case_id = case_claimed_lawyers.case_id AND cases_claimed.citizen_id = case_claimed_lawyers.citizen_id
where Cases.rule_number = NVL(:RollNumber, Cases.rule_number)
AND Cases.court_year = NVL(:CourtYears,Cases.court_year)
AND Cases.rule_subject_type_code= NVL(:RollType, Cases.rule_subject_type_code)
AND NVL(cases_claimed.CITIZEN_ID,-1) = NVL(:CaseClaimeds,NVL(cases_claimed.CITIZEN_ID,-1))
AND NVL(cases_defendant.CITIZEN_ID,-1) = NVL(:CaseDefendants,NVL(cases_defendant.CITIZEN_ID,-1)) 
AND NVL(case_claimed_lawyers.citizen_id,-1) = NVL(:ClaimedsLawers,NVL(case_claimed_lawyers.CITIZEN_ID,-1))
AND NVL(case_defendant_lawyers.citizen_id,-1) = NVL(:DefendantsLawers,NVL(case_defendant_lawyers.CITIZEN_ID,-1))]]>
  </SQLQuery>
