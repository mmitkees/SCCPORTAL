  <SQLQuery>
    <![CDATA[SELECT      Cases.rule_number,
   Cases.court_year,Cases.court_type_code,Cases.rule_subject_type_code,
getRulesSubjectTypeName(  Cases.rule_subject_type_code) as rule_subject_type_name,
COURT_ROLL_MOFEDEEN.roll_id,
GET_ROLL_MOFEDEEN_STATUS(COURT_ROLL_MOFEDEEN.Case_id,COURT_ROLL_MOFEDEEN.rule_date) as roll_status,
COURT_ROLL_MOFEDEEN.rule_date,
get_cases_claimeds_brief(Cases.case_id) as Claimeds ,
 get_cases_defendants_brief(Cases.case_id) as defendents,
 cases.subject, COURT_ROLL_MOFEDEEN.decree, get_judge_done_name_mofedden(COURT_ROLL_MOFEDEEN.judge_Done) judge_Done, COURT_ROLL_MOFEDEEN.Postpone_Date, 
 COURT_ROLL_MOFEDEEN.role_order, 
getcourtname(  Cases.court_type_code ) as court_type_name,Cases.Case_Status_CODE ,
getCaseStatusName( Cases.Case_Status_CODE) as status_name 
FROM         COURT_ROLL_MOFEDEEN INNER JOIN
                      Cases ON COURT_ROLL_MOFEDEEN.Case_id = Cases.case_id
where COURT_ROLL_MOFEDEEN.RULE_DATE = :RollDate AND GET_ROLL_MOFEDEEN_STATUS_CODE(COURT_ROLL_MOFEDEEN.Case_id,COURT_ROLL_MOFEDEEN.rule_date)=NVL(:RollStatus,GET_ROLL_MOFEDEEN_STATUS_CODE (COURT_ROLL_MOFEDEEN.Case_id,COURT_ROLL_MOFEDEEN.rule_date))
AND Cases.rule_subject_type_code= NVL(:RollType, Cases.rule_subject_type_code)]]>
  </SQLQuery>
