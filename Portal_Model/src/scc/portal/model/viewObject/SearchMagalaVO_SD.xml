  Name="SearchMagalaVO_SD"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="scc.portal.model.viewObject.SearchMagalaVO_SD_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="MagalaNum"
    Kind="where"
    Type="oracle.jbo.domain.Number">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="${adfBundle['SCC_MODEL_AR']['MagalaID']}"/>
