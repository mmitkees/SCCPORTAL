  <Variable
    Name="AuthorName"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="${adfBundle['SCC_MODEL_AR']['AUTHORNAME']}"/>
        <DISPLAYWIDTH
          Value="20"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery>
    <![CDATA[SELECT MAKAL.ID,
  MAKAL.NAMEAR,
  MAKAL.NAMEEN,
  MAKAL.MAKALURL,
  MAKAL.AUTHORNAME,
  MAKAL.MAGALAID,
  MAKAL.ORDERINMAGALA
FROM MAKAL join magala on makal.magalaid = magala.id
where magala.id= nvl(:MagalaNum,magala.id )
AND MAKAL.AUTHORNAME like '%' || :AuthorName || '%']]>
  </SQLQuery>
