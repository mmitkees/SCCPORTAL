  <SQLQuery>
    <![CDATA[SELECT cases.rule_number ,cases.court_year,
getrulessubjecttypename(cases.rule_subject_type_code) subjectTypeName,
getrulestatusname(cases.rule_status_code),
rules_insert.RULE_DATE,
rules_insert.RULE_NTEXT,
GETWARDINGNAME(rules_rules_warding.RULE_WARDING_CODE)  as WARDINGNAME,
GETRULES_KEY_NAME(rule_key.rule_key_code) as KEY_NAME
FROM RULES_INSERT join cases on rules_insert.case_id = cases.case_id
left outer join  rules_rules_warding on rules_insert .id = rules_rules_warding.id 
left outer join RULE_KEY on rules_insert.id = RULE_KEY .id
where cases.court_type_code = 1 and 
     cases.court_year= nvl(:courtyear,9) and 
     cases.rule_subject_type_code = nvl(:ruletype,cases.rule_subject_type_code) and
     cases.rule_status_code=nvl(:statuscode,cases.rule_status_code) and
     rules_insert.RULE_DATE between nvl(:datefrom,TO_DATE('25/05/1850','DD-MM-YYYY')) AND nvl(:dateto,TO_DATE('25/05/2100','DD-MM-YYYY')) and
     rules_rules_warding.RULE_WARDING_CODE = nvl(:rulewarding,rules_rules_warding.RULE_WARDING_CODE)]]>
  </SQLQuery>
